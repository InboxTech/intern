{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off",
        "aria-input-field-name": "off",
        "aria-required-children": "off"
      }
    ],
    "no-inline-styles": "off",
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "meta-viewport": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ]
  }
}